  </servlet>
  <servlet-mapping>
    <servlet-name>AddTaskController</servlet-name>
    <url-pattern>/AddTaskController</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>TaskValidationFilter</display-name>
    <filter-name>TaskValidationFilter</filter-name>
    <filter-class>TaskValidationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>TaskValidationFilter</filter-name>
    <servlet-name>AddTaskController</servlet-name>
  </filter-mapping>
</web-app>